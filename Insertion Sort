#include<stdio.h>

int main(){

   int i, j, n, temp, data[25];
   printf("=========> Program Insertion Sort <=========\n\n");

   printf("Berapa banyak data yang ingin dimasukkan?: ");
   scanf("%d",&n);

   printf("Masukkan %d data: \n", n);
   for(i=0;i<n;i++)
      scanf("%d",&data[i]);

   for(i=1;i<n;i++){
      temp=data[i];
      j=i-1;
      while((temp<data[j])&&(j>=0)){
         data[j+1]=data[j];
         j=j-1;
      }
      data[j+1]=temp;
   }

   printf("Menampilkan data yang di urutkan :\n");
   for(i=0;i<n;i++)
      printf("%d\n",data[i]);

   return 0;
}

#penjelasan
